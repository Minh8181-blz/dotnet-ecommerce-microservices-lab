{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MaktaShop": "<DB_CONN_STRING>",
    "QueueConnection": "<RABBIT_CONN_STRING>"
  },
  "Jwt": {
    "Issuer": "API.Identity",
    "SecretKey": "<MY_UNUSED_SECRET_KEY>",
    "ExpireSpan": 10
  },
  "IdentityOrigin": "https://localhost:6401"
}
